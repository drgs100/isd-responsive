<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <add value="index.asp" />
            </files>
        </defaultDocument>
        <staticContent>
              <remove fileExtension=".woff" />
              <mimeMap fileExtension=".woff" mimeType="application/x-font-woff" />
              <remove fileExtension=".woff2" />
              <mimeMap fileExtension=".woff2" mimeType="application/font-woff2" />
        </staticContent>
    </system.webServer>
    <connectionStrings>
        <add connectionString="server=localhost;uid=root;Pwd=root;database=ISDWeb" name="ISDWeb" providerName="MySql.Data.MySqlClient" />
    </connectionStrings>
</configuration>